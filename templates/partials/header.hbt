<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>
		CSS Brasil -

		{{#if this.name}}
			{{this.name}}
		{{/if}}

		{{#if this.title}}
			{{this.title}}
		{{/if}}
	</title>

	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<link rel="publisher" href="https://plus.google.com/u/0/107923948578708069647/posts"/>
	<meta property="og:locale" content="pt_BR" />
	<meta property="og:type" content="" />
	<meta property="og:title" content="{{#if this.name}}{{this.name}}{{/if}} {{#if this.title}}{{this.title}}{{/if}}" />
	<meta property="og:description" content="{{#if this.subtitle}}{{this.subtitle}}{{/if}}{{#if this.description}}{{this.description}}{{/if}}" />
	<meta property="og:site_name" content="CSS Brasil" />
	{{#if this.is_template_page}}
    <meta property="og:image" content="http://cssbrasil.org/images/css-brasil-logo.png" />
	{{else}}
	<meta property="og:image" content="http://cssbrasil.org{{this.featured}}" />
	{{/if}}
	{{#if this.is_template_page}}
	<meta name="twitter:image:src" content="http://cssbrasil.org/images/css-brasil-logo.png" />
	{{else}}
	<meta name="twitter:image:src" content="http://cssbrasil.org{{this.featured}}" />
	{{/if}}
	<meta name="twitter:title" content="{{#if this.name}}{{this.name}}{{/if}} {{#if this.title}}{{this.title}}{{/if}}"/>
	<meta name="twitter:site" content="@Clovissneto"/>
	<meta name="twitter:domain" content="CSS Brasil"/>

	<meta name="description" content="{{#if this.subtitle}}{{this.subtitle}}{{/if}}{{#if this.description}}{{this.description}}{{/if}}">
	<meta name="keywords" content="{{#if this.tags}}{{this.tags}}{{/if}}{{#if this.keywords}}{{this.keywords}}{{/if}}" />

	<link href="https://fonts.googleapis.com/css?family=Dosis:400|Indie+Flower|Roboto" rel="stylesheet">
	<link rel="stylesheet" href="/css/font-awesome.min.css">
	<link rel="stylesheet" href="/css/custom.css">
	<link rel="stylesheet" href="/css/main.css">
